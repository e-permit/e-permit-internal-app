{
    "build": {
        "beforeBuildCommand": "",
        "beforeDevCommand": "",
        "devPath": "./build",
        "distDir": "./build"
    },
    "package": {
        "productName": "e-permit-internal-app",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "http": {
                "all": true
            }
        },
        "bundle": {
            "identifier": "epermit.internal.app",
            "icon": [
                "public/icons/32x32.png",
                "public/icons/128x128.png",
                "public/icons/icon.ico"
            ]
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 700,
                "resizable": true,
                "title": "E-Permit Internal Application",
                "width": 768
            }
        ]
    }
}